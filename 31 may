// Aggregate Fun - Min, Max, Avg, Count, sum


select * from sales;



select count (order_id) as order_id, count (product_id) as product_id
from sales
where order_id = ('CA-2016-152156');


select count(order_line) as " No. of products orders", count(distinct order_id) as "No. of Orders"
from sales
where order_id = 'CA-2016-152156';
